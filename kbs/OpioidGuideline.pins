	(%3APAL-STATEMENT "(forall ?drug_usage \n        (=> (Drug_Class_Name ?drug_usage (coerce-to-class \"Alpha_blockers\"))\n            (not (exists ?med \n                         (or (exists ?drug_class \n                                     (and (Drug_Partners_To_Avoid ?drug_usage ?drug_class)\n                                          (equal-or-subclass-of (coerce-to-class (drug_name ?med)) ?drug_class)))\n                             (exists ?drug \n                                     (and (Drug_Partners_To_Avoid ?drug_usage ?drug)\n                          \n                                          (equal-or-subclass-of (coerce-to-class (drug_name ?med)) ?drug)))))\n            )\n        ))"))

