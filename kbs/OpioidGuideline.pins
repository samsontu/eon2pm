([athena_edit_138626] of  %3APAL-CONSTRAINT

	(%3APAL-NAME "Z NOT OT betablocker not contraindicated")
	(%3APAL-RANGE "(defrange ?condition :FRAME Note_Entry)\n(defrange ?problem :FRAME Medical_Conditions_Metaclass)\n(defrange ?derived_problem :FRAME Diagnostic_Term_Metaclass)\n(defrange ?drug_usage :FRAME Drug_Usage)")
	(%3APAL-STATEMENT "(forall ?drug_usage\n        (=> (Drug_Class_Name ?drug_usage Beta-Blockers-Cardioselective)\n            (and (not (exists ?condition \n                              (and (patient_id ?condition $patient_id)\n                                   (exists ?problem \n                                           (and (Absolute_Contraindications ?drug_usage ?problem)\n                                                (isa-classname (domain_term ?condition))\n                                                (or (= (coerce-to-class (domain_term ?condition)) ?problem )\n(subclass-of (coerce-to-class (domain_term ?condition)) ?problem )) \n                                   )))))\n                 (not (exists  ?derived_problem \n                               (and (Absolute_Contraindications ?drug_usage ?derived_problem)\n                                    (is-derivable ?derived_problem))))\n            ))\n)"))

