([ATHENA_HF_PM_Class10002] of  %3APAL-CONSTRAINT

	(%3APAL-NAME "ACEi within [-2, +1] day of discharge")
	(%3APAL-RANGE "(defrange ?med :FRAME Medication)\n(defrange ?hospitalizationTime :FRAME Definite_Time_Interval)")
	(%3APAL-STATEMENT "(exists ?med \n        (and (subclass-of (coerce-to-class (drug_name ?med)) ACE_Inhibitors)\n             (=(patient_id ?med) $patient_id)\n             (or (= (status ?med) \"active\")\n                 (exists ?hospitalizationTime\n                         (and (= (:NAME ?hospitalizationTime) \"FocusAdmissionPeriod\")\n                              (<  (system_time  (start_time  (valid_time ?med)))(+ 2 (system_time (stop_time ?hospitalizationTime))))\n                              (>  (system_time  (start_time  (valid_time ?med)))(-  (system_time (stop_time ?hospitalizationTime)) 3))\n                         )\n                 )\n             )\n        )\n)"))

