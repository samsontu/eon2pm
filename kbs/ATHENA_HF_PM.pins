([KB_697275_Class0] of  %3APAL-CONSTRAINT

	(%3APAL-DESCRIPTION "Any increase of more than 20% in creatinine AFTER prescription for an ACEi/ARB.  The comparison is FROM the baseline creatinine prior to initiation of ACEi/ARB (the most recent creatinine prior to first ACEi/ARB) TO the peak creatinine within 3 months after initiation of ACEi/ARB (MKG 2012)")
	(%3APAL-NAME "20% increase in creatinine after ACE/ARB")
	(%3APAL-RANGE "(defrange ?baseCreat :FRAME Numeric_Entry)\n(defrange ?peakCreat :FRAME Numeric_Entry)\n(defrange ?initACEARB :FRAME Medication)\n(defrange ?preCreat :FRAME Numeric_Entry)\n(defrange ?postCreat :FRAME Numeric_Entry)\n(defrange ?nonACEARB :FRAME Medication)\n")
	(%3APAL-STATEMENT "(exists ?initACEARB (and (= (patient_id ?initACEARB) $patient_id)\n                         (or (subclass-of (coerce-to-class (drug_name ?initACEARB)) ACE_Inhibitors)\n                             (subclass-of (coerce-to-class (drug_name ?initACEARB)) Angiotensin_II_receptor_blockers))\n                         (not (exists ?nonACEARB (and (= (patient_id ?nonACEARB) $patient_id)\n                                                      (or (subclass-of (coerce-to-class (drug_name ?nonACEARB)) ACE_Inhibitors)\n                                                          (subclass-of (coerce-to-class (drug_name ?nonACEARB)) Angiotensin_II_receptor_blockers))\n                                                      (>(system_time (start_time (valid_time ?initACEARB)))(system_time (start_time (valid_time ?nonACEARB)))))))\n                         (exists ?baseCreat (and (= (patient_id ?baseCreat) $patient_id)\n                                                 (=    (domain_term ?baseCreat) \"Creatinine\")\n                                                 (< (system_time (valid_time ?baseCreat)) (system_time (start_time (valid_time ?initACEARB))))\n                                                 (not (exists ?preCreat (and (= (patient_id ?preCreat) $patient_id)\n                                                                             (=    (domain_term ?preCreat) \"Creatinine\")\n                                                                             (< (system_time (valid_time ?preCreat)) (system_time (start_time (valid_time ?initACEARB))))\n                                                                             (> (system_time (valid_time ?preCreat)) (system_time (valid_time ?baseCreat))))))\n                                                 (exists ?peakCreat (and (= (patient_id ?peakCreat) $patient_id)\n                                                                         (=    (domain_term ?peakCreat) \"Creatinine\")                                                                \n                                                                         (> (system_time (valid_time ?peakCreat)) (system_time (start_time (valid_time ?initACEARB))))\n                                                                         (< (system_time (valid_time ?peakCreat)) (+ 90 (system_time (start_time (valid_time ?initACEARB)))))\n                                                                         (not (exists ?postCreat  (and (= (patient_id ?postCreat) $patient_id)\n                                                                                                       (=    (domain_term ?postCreat) \"Creatinine\")                                                                \n                                                                                                       (> (system_time (valid_time ?postCreat)) (system_time (start_time (valid_time ?initACEARB))))\n                                                                                                       (> (value ?postCreat)(value ?peakCreat)))))\n                                                                         (> (/ (* 100 (- (value ?peakCreat) (value ?baseCreat))) (value ?peakCreat)) 20)\n                                                                    ))\n                                            ))\n                    ))"))

